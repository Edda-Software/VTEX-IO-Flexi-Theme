{
    "flex-layout.row#block-four_oportunity": {
        "props": {
            "blockClass": "block-four-oportunity-container"
        },
        "children": [
            "flex-layout.col#block-four-oportunity"
        ]
    },
    "flex-layout.col#block-four-oportunity": {
        "props": {
            "blockClass": "block-four-oportunity"
        },
        "children": [
            "rich-text#block-four-oportunity-title",
            "responsive-layout.mobile#block-four-oportunity",
            "responsive-layout.desktop#block-four-oportunity"
        ]
    },
    "rich-text#block-four-oportunity-title": {
        "props": {
            "blockClass": "block-four-oportunity-title",
            "text": "Grandes oportunidades"
        }
    },
    /* MOBILE */
    "responsive-layout.mobile#block-four-oportunity": {
        "children": [
            "flex-layout.row#block-four-oportunity-images-mobile"
        ]
    },
    "flex-layout.row#block-four-oportunity-images-mobile": {
        "props": {
            "blockClass": "block-four-oportunity-images-mobile",
            "colSizing": "auto"
        },
        "children": [
            "flex-layout.col#block-four-oportunity-images-mobile-1",
            "flex-layout.col#block-four-oportunity-images-mobile-2",
            "flex-layout.col#block-four-oportunity-images-mobile-3"
        ]
    },
