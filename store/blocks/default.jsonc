{
  /* BREADCRUMB */
  "breadcrumb.search": {
    "props": {
      "showOnMobile": true
    }
  },
  "breadcrumb": {
    "props": {
      "showOnMobile": true
    }
  },
  /* ORDERPLACED */
  "store.orderplaced": {
    "blocks": [
      "order-placed"
    ]
  },
  /* ACCOUNT */
  "store.account": {
    "blocks": [
      "my-account"
    ],
    "parent": {
      "challenge": "challenge.profile"
    }
  },
  /* LOGIN */
  "store.login": {
    "blocks": [
      "login-content#default"
    ]
  },
  "login-content#default": {
    "props": {
      "isInitialScreenOptionOnly": false,
      "defaultOption": 1,
      "showPasswordVerificationIntoTooltip": true
    }
  },
  /* PRODUCT SUMMARY */
  "product-summary.shelf#global": {
    "props": {
      "blockClass": "product-summary-shelf-container"
    },
    "children": [
      "product-summary-image",
      "product-summary-name",
      "product-summary-description",
      "flex-layout.row#product-price_shelf-global",
      "modal-trigger#quick-purshase"
    ]
  },
  "flex-layout.row#product-price_shelf-global": {
    "props": {
      "blockClass": "product-price_shelf-global",
      "colSizing": "auto",
      "colJustify": "left"
    },
    "children": [
      "product-selling-price",
      "product-list-price"
    ]
  },
  "modal-trigger#quick-purshase": {
    "children": [
      "rich-text#quick-purshase",
      "modal-layout#quick-purshase"
    ]
  },
  "rich-text#quick-purshase": {
    "props": {
      "blockClass": "quick-purshase-button",
      "text": "Agregar a carrito"
    }
  },
  "modal-layout#quick-purshase": {
    "props": {
      "blockClass": "quick-purshase"
    },
    "children": [
      "modal-header",
      "flex-layout.row#product-page_container-summary-product"
    ]
  },
  /* SLIDER LAYOUT SUMMARY PRODUCT */
  "slider-layout#carousel-banner-product-summary": {
    "props": {
      "blockClass": "carousel-banner-product-summary",
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 3,
        "phone": 2
      },
      "infinite": false,
      "showNavigationArrows": "never",
      "usePagination": false
    }
  },
  /* SLIDER LAYOUT IMAGES */
  "slider-layout#carousel-banner-image": {
    "props": {
      "blockClass": "carousel-banner-image",
      "showNavigationArrows": "never",
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true,
      "fullWidth": true
    }
  },
  /* NEWSLETTER */
  "flex-layout.row#newsletter-global-container": {
    "props": {
      "blockClass": "newsletter-global-container",
      "colSizing": "auto"
    },
    "children": [
      "flex-layout.col#newsletter-global-contain",
      "responsive-layout.mobile#newsletter-global-image",
      "responsive-layout.desktop#newsletter-global-image"
    ]
  },
  "flex-layout.col#newsletter-global-contain": {
    "props": {
      "blockClass": "newsletter-global-contain",
      "verticalAlign": "middle"
    },
    "children": [
      "flex-layout.row#newsletter-global-contain-titles-text",
      "newsletter-form#global"
    ]
  },
  "flex-layout.row#newsletter-global-contain-titles-text": {
    "props": {
      "blockClass": "newsletter-global-contain-titles-text"
    },
    "children": [
      "flex-layout.col#newsletter-global-contain-titles-text"
    ]
  },
  "flex-layout.col#newsletter-global-contain-titles-text": {
    "children": [
      "rich-text#newsletter-global-title",
      "rich-text#newsletter-global-subtitle"
    ]
  },
  "rich-text#newsletter-global-title": {
    "props": {
      "blockClass": "newsletter-global-title",
      "text": "Suscríbete a nuestra newsletter"
    }
  },
  "rich-text#newsletter-global-subtitle": {
    "props": {
      "blockClass": "newsletter-global-subtitle",
      "text": "Entérate de primera mano lo que tenemos para ti"
    }
  },
  "newsletter-form#global": {
    "children": [
      "flex-layout.row#newsletter-global-content-inputs",
      "newsletter-checkbox-confirmation#global"
    ]
  },
  "flex-layout.row#newsletter-global-content-inputs": {
    "props": {
      "blockClass": "newsletter-global-content-inputs",
      "colSizing": "auto"
    },
    "children": [
      "newsletter-input-email#global",
      "newsletter-submit#global"
    ]
  },
  "newsletter-input-email#global": {
    "props": {
      "placeholderText": "Correo"
    }
  },
  "newsletter-submit#global": {
    "props": {
      "submitButtonLabel": "Suscribirme"
    }
  },
  "newsletter-checkbox-confirmation#global": {
    "props": {
      "checkboxLabel": "He leído y acepto las Políticas de Privacidad y Tratamiento de Datos Personales. ",
      "firstLabelLink": {
        "url": "/terminos-condiciones",
        "text": "Ver políticas aquí"
      }
    }
  },
  "responsive-layout.mobile#newsletter-global-image": {
    "children": [
      "image#newsletter-global-image-mobile"
    ]
  },
  "responsive-layout.desktop#newsletter-global-image": {
    "children": [
      "image#newsletter-global-image-desktop"
    ]
  },
  "image#newsletter-global-image-mobile": {
    "props": {
      "blockClass": "newsletter-global-image-mobile",
      "src": "assets/images/generalComponents/newsletter-mobile.jpg"
    }
  },
  "image#newsletter-global-image-desktop": {
    "props": {
      "src": "assets/images/generalComponents/newsletter-desktop.png"
    }
  }
}